<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core"
controllerName="com.tcs.hr.ui.project1.controllers.masterData"
xmlns="sap.m"
> 
   <Page title="Master Data" showNavButton="true" navButtonPress=".onBack">
        <FlexBox alignItems="Center" justifyContent="Center" width="100%" height="100%" >
                <Select id="typeSelect" items="{
                    path: '/requestArr'
                }" change=".typeChanged">
                    <core:Item key="{key}" text="{text}" />
                </Select>
            </FlexBox>
   </Page>
</mvc:View>