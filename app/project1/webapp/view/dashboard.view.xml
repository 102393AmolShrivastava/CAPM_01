<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" controllerName="com.tcs.hr.ui.project1.controllers.dashboard"
    xmlns="sap.m"> 
        <Page id="dashboardScreen" title="Dashboard" showNavButton="true" navButtonPress=".onBack"> 
            <FlexBox alignItems="Center" justifyContent="Center" width="100%" height="100%">
                <Select id="typeSelect" items="{
                    path: '/requestArr'
                }" change=".typeChanged">
                    <core:Item key="{key}" text="{text}" />
                </Select>
            </FlexBox>
        </Page>
</mvc:View>